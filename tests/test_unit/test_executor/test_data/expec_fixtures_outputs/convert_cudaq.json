  "circuit_with_boolean_logic_condition": {
    "program": [
      "q = cudaq.qvector(3)",
      "# Start of circuit: circuit_with_advanced_condition",
      "h(q[0])",
      "x(q[1])",
      "c0 = mz(q[0], regName='c0')",
      "c1 = mz(q[1], regName='c1')",
      "if (c0 and c1):",
      "    x(q[2])",
      "else:",
      "    z(q[2])",
      "if (not (c0 and c1)):",
      "    x(q[2])",
      "else:",
      "    z(q[2])",
      "if (c0 or c1):",
      "    x(q[2])",
      "else:",
      "    z(q[2])",
      "if (not (c0 or c1)):",
      "    x(q[2])",
      "else:",
      "    z(q[2])",
      "if (c0 ^ c1):",
      "    x(q[2])",
      "else:",
      "    z(q[2])",
      "if (not c0):",
      "    x(q[2])",
      "else:",
      "    z(q[2])",
      "if (c0):",
      "    x(q[2])",
      "else:",
      "    z(q[2])",
      "c2 = mz(q[2], regName='c2')",
      "#  End of circuit: circuit_with_advanced_condition"
    ]
  },
  "circuit_w_nested_if_else": {
    "program": [
      "q = cudaq.qvector(2)",
      "# Start of circuit: circuit_with_nested_if_else",
      "h(q[0])",
      "c0 = mz(q[0], regName='c0')",
      "c1 = mz(q[1], regName='c1')",
      "if (c0):",
      "    if (c1):",
      "        x(q[1])",
      "    else:",
      "        y(q[1])",
      "else:",
      "    z(q[1])",
      "c2 = mz(q[1], regName='c2')",
      "#  End of circuit: circuit_with_nested_if_else"
    ]
  },
  "circuit_with_boolean_logic_condition_multibit": {
    "program": [
