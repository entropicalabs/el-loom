      "x q[1];",
      "measure q[0] -> c[0];",
      "measure q[0] -> c[1];",
      "measure q[0] -> c[2];",
      "measure q[0] -> c[3];",
      "measure q[0] -> c[4];",
      "if (!(c[0] == 1 && c[1] == 1 && c[2] == 1 && c[3] == 1 && c[4] == 1)) {",
      "    x q[2];",
      "} else {",
      "    z q[2];",
      "}",
      "measure q[2] -> c[5];",
      "//  End of circuit: circuit_with_advanced_condition_multibit"
    ]
  },
