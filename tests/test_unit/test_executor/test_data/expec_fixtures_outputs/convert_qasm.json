  "circuit_with_simple_if_else": {
    "program": [
      "OPENQASM 3.0;",
      "include \"stdgates.inc\";",
      "// Init registers",
      "qubit[2] q;",
      "bit[2] c;",
      "// Start of circuit: circuit_with_if_else",
      "h q[0];",
      "measure q[0] -> c[0];",
      "if (c[0]) {",
      "    x q[1];",
      "} else {",
      "    z q[1];",
      "}",
      "measure q[1] -> c[1];",
      "//  End of circuit: circuit_with_if_else"
    ]
  },
