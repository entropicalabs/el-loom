  "circuit_with_boolean_logic_condition": {
    "program": [
      "OPENQASM 3.0;",
      "include \"stdgates.inc\";",
      "// Init registers",
      "qubit[3] q;",
      "bit[3] c;",
      "// Start of circuit: circuit_with_advanced_condition",
      "h q[0];",
      "x q[1];",
      "measure q[0] -> c[0];",
      "measure q[1] -> c[1];",
      "if (c[0] == 1 && c[1] == 1) {",
      "    x q[2];",
      "} else {",
      "    z q[2];",
      "}",
      "if (!(c[0] == 1 && c[1] == 1)) {",
      "    x q[2];",
      "} else {",
      "    z q[2];",
      "}",
      "if (c[0] == 1 || c[1] == 1) {",
      "    x q[2];",
      "} else {",
      "    z q[2];",
      "}",
      "if (!(c[0] == 1 || c[1] == 1)) {",
      "    x q[2];",
      "} else {",
      "    z q[2];",
      "}",
      "if (c[0] == 1 ^ c[1] == 1) {",
      "    x q[2];",
      "} else {",
      "    z q[2];",
      "}",
      "if (!c[0]) {",
      "    x q[2];",
      "} else {",
      "    z q[2];",
      "}",
      "if (c[0]) {",
      "    x q[2];",
      "} else {",
      "    z q[2];",
      "}",
      "measure q[2] -> c[2];",
      "//  End of circuit: circuit_with_advanced_condition"
    ]
  },
  "circuit_w_nested_if_else": {
    "program": [
      "OPENQASM 3.0;",
      "include \"stdgates.inc\";",
      "// Init registers",
      "qubit[2] q;",
      "bit[3] c;",
      "// Start of circuit: circuit_with_nested_if_else",
      "h q[0];",
      "measure q[0] -> c[0];",
      "measure q[1] -> c[1];",
      "if (c[0]) {",
      "    if (c[1]) {",
      "        x q[1];",
      "    } else {",
      "        y q[1];",
      "    }",
      "} else {",
      "    z q[1];",
      "}",
      "measure q[1] -> c[2];",
      "//  End of circuit: circuit_with_nested_if_else"
    ]
  },
